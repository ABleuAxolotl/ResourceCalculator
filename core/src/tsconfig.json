{
	"compilerOptions": {
		"outDir": "../../cache",
		"rootDir": ".",
		"sourceMap": true,
		"module": "commonjs",
		"target": "es5",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"strict": true,
		"downlevelIteration": true,
		"noImplicitAny": true,
		"strictNullChecks": true
	},
	"include": ["*.ts"]
}
